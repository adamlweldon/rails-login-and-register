<h1>Our Users</h1>
<%= link_to 'Logout', signout_path, :class=> "btn btn-danger"%>
<table class="table">
	<tr>
		<th>Name</th> 
		<th>Email</th>
		<th>Options</th>
	</tr>
	<% @user.each do |user| %>
	<tr>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= link_to 'Go to Profile', user, :class=> "btn btn-primary"%></td>
	</tr>
	<% end %>
</table>
